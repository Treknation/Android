<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.treknation.myapplicationclone"
    android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Dashboard"></activity>
        <activity android:name=".OutsideCanada" />
        <activity android:name=".InCanada" />
        <activity android:name=".CicContact" />
        <activity android:name=".PrivacyPolicy" />
        <activity android:name=".Disclaimer" />
        <activity android:name=".Feedback" />
        <activity android:name=".AboutUs" />
        <activity android:name=".MoreActivity" />
        <activity android:name=".WebsiteActivity" />
        <activity android:name=".Fsw" />
        <activity android:name=".Fst" />
        <activity android:name=".Cec" />
        <activity android:name=".LanguageCosts" />
        <activity android:name=".ConvertLanguageResults" />
        <activity android:name=".Submit_ee" />
        <activity android:name=".Costs" />
        <activity android:name=".LongDesc" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>