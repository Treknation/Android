<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MoreActivity"
    android:id="@+id/more_activity">

    <ImageView
        android:id="@+id/home_logo"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="17dp"
        android:scaleType="fitXY"
        android:src="@drawable/home_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtTrekNation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/havania"
        android:text="TrekNation"
        android:textColor="#143d59"
        android:textSize="32sp"
        app:layout_constraintStart_toEndOf="@+id/home_logo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtAboutUs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/sourcesansprosemibold"
        android:text="About Us"
        android:textColor="#143d59"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTrekNation" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="13dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="36dp"
        android:background="#e9e9e9"
        app:layout_constraintTop_toBottomOf="@+id/txtAboutUs"
        tools:layout_editor_absoluteX="36dp" />

    <TextView
        android:id="@+id/txtFeedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="7dp"
        android:text="Feedback"
        android:fontFamily="@font/sourcesansprosemibold"
        android:textColor="#143d59"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="13dp"
        android:background="#e9e9e9"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintTop_toBottomOf="@+id/txtFeedback"
        tools:layout_editor_absoluteX="36dp" />

    <TextView
        android:id="@+id/txtDisclaimer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesansprosemibold"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="7dp"
        android:text="Disclaimer"
        android:textColor="#143d59"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="13dp"
        android:background="#e9e9e9"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintTop_toBottomOf="@+id/txtDisclaimer"
        tools:layout_editor_absoluteX="36dp" />

    <TextView
        android:id="@+id/txtPrivacyPolicy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesansprosemibold"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="7dp"
        android:text="Privacy Policy"
        android:textColor="#143d59"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="13dp"
        android:background="#e9e9e9"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintTop_toBottomOf="@+id/txtPrivacyPolicy"
        tools:layout_editor_absoluteX="36dp" />

    <TextView
        android:id="@+id/txtCicContactInformation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesansprosemibold"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="7dp"
        android:text="CIC Contact Information"
        android:textColor="#143d59"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="13dp"
        android:background="#e9e9e9"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintTop_toBottomOf="@+id/txtPrivacyPolicy"
        tools:layout_editor_absoluteX="36dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="Â© TrekNation Technologies Ltd."
        android:textColor="#46494c"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="V 1.0"
        android:textColor="#46494c"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>